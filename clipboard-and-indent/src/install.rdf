<?xml version="1.0" encoding="UTF-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
 xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>clipboard-and-indent@selite.googlecode.com</em:id>
    <em:name>SeLite Clipboard And Indent</em:name>
    <em:version>0.12</em:version>
    <em:creator>Peter Kehl</em:creator>
    <em:homepageURL>https://code.google.com/p/selite/</em:homepageURL>
    <em:description>It makes Selenium IDE accept HTML from clipboard (e.g. from another Selenium IDE instance). It enables Selenese commands to be indented with spaces into blocks. It indents and de-indents structured commands that come with SeLite SelBlocks Global. </em:description>
    <em:type>2</em:type> <!--Type extension-->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id> <!-- Firefox -->
        <em:minVersion>27.0</em:minVersion>
        <em:maxVersion>38.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
