<?xml version="1.0" encoding="UTF-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
 xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>run-all-favorites@selite.googlecode.com</em:id>
    <em:name>SeLite Run All Favorites</em:name>
    <em:version>0.12</em:version>
    <em:creator>Peter Kehl</em:creator>
    <em:description>It runs all favorites from "Favorites (Selenium IDE)". They are stored relative to user's home folder. It lets the user to import and export favorites through the menu.</em:description>
    <em:type>2</em:type> <!--Type extension-->
    <em:homepageURL>http://selite.github.io/</em:homepageURL>
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id> <!-- Firefox -->
        <em:minVersion>27.0</em:minVersion>
        <em:maxVersion>44.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    <em:iconURL>chrome://selite-run-all-favorites/skin/favicon-32x32.png</em:iconURL>
    <em:icon64URL>chrome://selite-run-all-favorites/skin/favicon-64x64.png</em:icon64URL>
  </Description>
</RDF>
