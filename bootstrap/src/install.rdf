<?xml version="1.0" encoding="UTF-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
 xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>bootstrap@selite.googlecode.com</em:id>
    <em:name>SeLite Bootstrap</em:name>
    <em:version>0.96</em:version>
    <em:creator>Peter Kehl</em:creator>
    <em:description>It reloads your Selenium Core extension (JS) automatically, whenever changed. You don't need to restart Selenium IDE everytime you change a Core extension that you develop.
    </em:description>
    <em:type>2</em:type> <!--Type extension-->
    <em:homepageURL>http://selite.github.io/</em:homepageURL>
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id> <!-- Firefox -->
        <em:minVersion>44.0</em:minVersion>
        <em:maxVersion>50.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    <em:iconURL>chrome://selite-bootstrap/skin/favicon-32x32.png</em:iconURL>
    <em:icon64URL>chrome://selite-bootstrap/skin/favicon-64x64.png</em:icon64URL>
  </Description>
</RDF>
