{

  "manifest_version": 2,
  "name": "SeLite Miscellaneous",
  "homepage_url": "http://selite.github.io/",
  "version": "1.11",

  "applications": {
    "gecko": {
      "id": "misc@selite.googlecode.com"
    }
  },
  
  "description": "Miscellaneous classes for SeLite components",

    "background": {
      "scripts": ["shared/migrating_to_webextensions.js", "shared/background.js"]
    },

    "permissions": [
      "management", "storage"
    ],

    "icons": {
      "32": "shared/favicon32x32.png",
      "64": "shared/favicon64x64.png"
    }
}
